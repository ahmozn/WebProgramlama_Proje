@model webProgProje.Models.Hasta

@{
    TempData["Title"] = "Hesap";
}

<h1>Hesap</h1>

<div>
    <h4>Hasta</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Kullanici)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Kullanici.TC)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="5">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
<div>
    @if (Model == null)
    {
        <h2>model boş</h2>
    }
    else
    {
        @if (Model.AktifRandevular == null)
        {
            <h3>Aktif randevunuz bulunmamaktadır.</h3>
        }
        else
        {
            foreach (var item in Model.AktifRandevular)
            {
                <h3>@item</h3>
            }
        }
    }
</div>
